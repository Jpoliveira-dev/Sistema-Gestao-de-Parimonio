    "module": "ES2022",
    "moduleResolution": "Bundler",
    "types": ["node"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "outDir": "dist",
    "rootDir": "./",
    "resolveJsonModule": true
